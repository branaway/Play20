rpath=`dirname $0`
echo real path: $rpath

if [ -f conf/application.yml ]
then
	$rpath/framework/build play "$@"
else
	java -cp $rpath/framework/sbt/boot/scala-2.9.0/lib/*:$rpath/framework/sbt/boot/scala-2.9.0/org.scala-tools.sbt/sbt/0.10.1/*:$rpath/repository/play/play_2.9.0/2.0/jars/* play.console.Console "$@"
fi